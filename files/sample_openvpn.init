start_instance() {
	local s="$1"

	config_get config "$s" config
	config="${config:+$(readlink -f "$config")}"
	
	# openvpn-ubus

	config_get management "$1" management
	config_get enabled "$1" enabled

	if [ -n "$management" ] && [ $enabled -gt 0 ]; then                                    
		/etc/init.d/openvpn-ubus start "$1" "$management"   
    fi

	# openvpn-ubus

    ...
}